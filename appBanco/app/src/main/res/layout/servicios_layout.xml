<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:background="#990000"
        app:srcCompat="@drawable/banorte" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Servicios" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Servicios de despensa" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="109dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="157dp"
            android:layout_height="89dp"
            android:onClick="btnWalmartClick"
            app:srcCompat="@drawable/walmart_logo_5" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="161dp"
            android:layout_height="77dp"
            app:srcCompat="@drawable/soriana" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="79dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/png_transparent_logo_brand_green_bodega_aurrera_product_line_text_label_rectangle" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView44"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Plataformas de streaming" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="96dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            app:srcCompat="@drawable/_68px_spotify" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="124dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            app:srcCompat="@drawable/png_transparent_netflix_macos_bigsur_icon" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="237dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            app:srcCompat="@drawable/logotipo_izzi_negativo" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="129dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="252dp"
            android:layout_height="113dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/hbo_max_logo" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="116dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/disney__logo_svg" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView45"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Servicios de television e internet" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="325dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="wrap_content"
            android:layout_height="117dp"
            android:layout_weight="1"
            android:onClick="btnMegacableClick"
            app:srcCompat="@drawable/png_transparent_megacable_hd_logo_thumbnail" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="wrap_content"
            android:layout_height="127dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/kisspng_logo_total_play_telecomunicaciones_s_a_de_c_v_m_esperanza_azteca_5be82cfd67be84_2595713215419425254249" />
    </LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        android:onClick="btnAtrasServiciosClick"
        android:text="Ir al menu principal" />

</LinearLayout>